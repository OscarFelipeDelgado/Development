<%- include("partials/_header") %>

<div class="container mt-5">
 
    <div class="col-md-5">
      <div class="card-body">
        <form action="/add" method="POST" id="signupForm">
          <input
            type="number"
            name="id"
            placeholder="Id"
            class="form-control mb-2"
          />
          <!-- <input 
            type="number"
            name="role_id"
            placeholder="Role_id"
            class="form-control mb-2"
            -->
          />
          <input
            type="text"
            name="name"
            placeholder="Name"
